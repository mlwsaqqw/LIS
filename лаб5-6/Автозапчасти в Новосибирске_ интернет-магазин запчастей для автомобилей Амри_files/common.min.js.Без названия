(()=>{function stickyHeader(){var containerSearchTable=document.querySelectorAll(".search-results__header");containerSearchTable&&containerSearchTable.forEach((function(item){var rows=item.querySelectorAll(".search-row");rows&&rows.length>=7&&item.classList.add("row-header_sticky")}))}document.addEventListener("DOMContentLoaded",(function(){stickyHeader(),window.EventsBus.on("search.initResults",(function(){stickyHeader()})),function(){var printModal=new DialogModal({templateId:"print-doc-modal"});if(printModal){var openPrintModalBtn=document.querySelector(".order-success__open-modal");openPrintModalBtn&&openPrintModalBtn.addEventListener("click",(function(){printModal.show()}))}}()}))})();